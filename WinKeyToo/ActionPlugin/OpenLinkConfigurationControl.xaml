<UserControl x:Class="WinKeyToo.ActionPlugin.OpenLinkConfigurationControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="3" Text="Hyperlink"/>
            <TextBox MinWidth="100" Margin="3" Text="{Binding Path=Hyperlink, Mode=TwoWay}" MaxWidth="200"/>
        </StackPanel>
    </Grid>
</UserControl>
